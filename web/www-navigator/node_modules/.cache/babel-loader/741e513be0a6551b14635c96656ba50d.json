const routes = [\n  {\n    path: \"/\",\n    name: \"Home\",\n    component: Home\n  },\n  {\n    path: \"/topic/:name\",\n    name: \"topicDetails\",\n    component: () => import(/* webpackChunkName: \"TopicDetails\" */\"../views/TopicDetails\"),\n    props: true,\n    children: [\n      {\n        path: \":subtopicName\",\n        name: \"subtopicDetails\",\n        props: true,\n        component: () => import(/* webpackChunkName: \"SubtopicDetails\" */\"../views/SubtopicDetails\"),\n      }\n    ],\n    meta: { requiresAuth: true },\n    beforeEnter: (to, from, next) => {\n      const exists = data.topics.find(\n        topic => topic.name === to.params.name\n      );\n      if (exists) {\n        next();\n      }\n      else {\n        next({ name: \"notFound\" });\n      }\n    }\n  },\n  {\n    path: \"/login\",\n    name: \"login\",\n    component: () => import(/* webpackChunkName: \"login\" */\"../views/Login\"),\n  },\n    {\n    path: \"/register\",\n    name: \"register\",\n    component: () => import(/* webpackChunkName: \"register\" */\"../views/Register\"),\n  },\n  {\n    path: \"/user\",\n    name: \"user\",\n    component: () => import(/* webpackChunkName: \"user\" */\"../views/User\"),\n    meta: {requiresAuth: true },\n  },\n  {\n    path: \"/404\",\n    alias: \"*\",\n    name: \"notFound\",\n    component: () => import(/* webpackChunkName: \"notFound\" */\"../views/NotFound\"),\n  }\n];\n\nconst router = new VueRouter({\n  mode: \"history\",\n  routes\n});\n\nrouter.beforeEach((to, from, next) => {\n  if (to.matched.some(record => record.meta.requiresAuth)) {\n    //need to login\n    if (!data.user) {\n      next({\n        name: \"login\",\n        query: {redirect: to.fullPath}\n      });\n    } else {\n      next();\n    }\n  } else {\n    next();\n  }\n});\n\nexport default router;\n"]}]}